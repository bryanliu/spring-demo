<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bry.coffeeshopmybatisgenerator.mapper.CoffeeRepo">
    <select id="searchByNameinXMl" resultMap="com.bry.coffeeshopmybatisgenerator.mapper.auto.CoffeeMapper.BaseResultMap">
        select
        <include refid="com.bry.coffeeshopmybatisgenerator.mapper.auto.CoffeeMapper.Base_Column_List"/>
        from T_COFFEE
        where NAME=#{name,jdbcType=VARCHAR}
    </select>
</mapper>
